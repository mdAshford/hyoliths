BEGIN MRBAYES;
  set autoclose=yes nowarn=yes; 
  
  lset coding=variable rates=gamma;
  prset brlenspr = unconstrained: gammadir(1, 0.33, 1, 1);

  charset Neomorphic = 1 2 3 5 7 8 9 10 12 14 17 18 19 22 23 24 25 28 29 30 33 35 37 44 45 47 48 49 50 51 52 55 59 60 61 62 64 65 69 70 72 73 75 78 79 83 84 85 86 87 89 90 91 94 95 97;
  charset Transformational = 4 6 11 13 15 16 20 21 26 27 31 32 34 36 38 39 40 41 42 43 46 53 54 56 57 58 63 66 67 68 71 74 76 77 80 81 82 88 92 93 96 98 99;
  partition chartype2 = 2: Neomorphic, Transformational;
  
  set partition = chartype2;
  
  unlink shape=(all) pinvar=(all);
  
  prset applyto=(1) symdirihyperpr=fixed(1.0);
  prset applyto=(1) ratepr=variable;
  
  outgroup Dailyatia;
  
  mcmcp ngen=1000000 samplefreq=500 nruns=4 nchains=8 burninfrac=0.1;

  mcmc printfreq=1000 checkfreq=100000 append=no;
  sumt burninf=0.1;
  sump burninf=0.1;  
END;
